{
  "name": "template-nexts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "git-commit": "git add . && git cz"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{css,less,html}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.4.0",
    "bdsaas-bc": "^0.0.6",
    "blocks-next": "1.0.1-alpha.32",
    "dayjs": "^1.11.9",
    "element-plus": "^2.3.8",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "pdd": "^0.3.23",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@dylan66ty/eslint-config-vue": "^0.0.8",
    "@dylan66ty/prettier-config": "^0.0.2",
    "@dylan66ty/stylelint-config": "^0.0.8",
    "@types/lodash-es": "^4.17.8",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.4.5",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "cross-env": "^7.0.3",
    "eslint": "8.33.0",
    "husky": "^8.0.3",
    "prettier": "2.8.3",
    "sass": "^1.64.1",
    "stylelint": "^15.10.2",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-define-options": "^1.3.12",
    "vite": "^4.4.5",
    "vite-plugin-vue-type-imports": "^0.2.5",
    "vue-tsc": "^1.8.5"
  }
}
